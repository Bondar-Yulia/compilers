let
    function fibonacci(n : int) : int =
        let
            var a := 1
            var b := 1
            var temp := 0
        in
            for i := 2 to n do
                (
                    temp := a + b;
                    a := b;
                    b := temp
                );
            b
        end
in
    for i := 0 to 7 do
        (
            print_int(fibonacci(i));
            print("\n")
        )
end